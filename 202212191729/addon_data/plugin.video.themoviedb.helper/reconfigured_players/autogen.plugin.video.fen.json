{
    "name"              : "FEN",
    "plugin"            : "plugin.video.fen",
    "priority"          : 100,
    "assert"            : {
                            "play_movie":       ["title", "year"],
                            "play_episode":     ["showname", "season", "episode"],
                            "search_movie":     ["title"],
                            "search_episode":   ["showname"]
                          },
    "play_movie"        : [
                            "plugin://plugin.video.fen/?action=tmdb_movies_search&amp;iconImage=player&amp;mode=build_movie_list&amp;query={title_url}",
                            {"title": "(?i)^(\\[.*\\])?.*{title}.*(\\[.*\\])?$", "year": "{year}"}
                          ],
    "play_episode"      : [
                            "plugin://plugin.video.fen/?action=tmdb_tv_search&amp;iconImage=player&amp;mode=build_tvshow_list&amp;query={showname_url}",
                            {"title": "(?i)^(\\[.*\\])?{showname}(\\[.*\\])?$"},
                            {"return": "true", "season": "^{season}$", "episode": "^{episode}$"},
                            {"season": "^{season}$"},
                            {"season": "^{season}$", "episode": "^{episode}$"}
                          ],
    "search_movie"      : [
                            "plugin://plugin.video.fen/?action=tmdb_movies_search&amp;iconImage=player&amp;mode=build_movie_list&amp;query={title_url}",
                            {"keyboard": "{title}"}
                          ],
    "search_episode"    : [
                            "plugin://plugin.video.fen/?action=tmdb_tv_search&amp;iconImage=player&amp;mode=build_tvshow_list&amp;query={showname_url}",
                            {"keyboard": "{showname}"}
                          ]
}